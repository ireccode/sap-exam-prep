{
  "name": "sap-architect-exam-prep",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "do-build": "npm install --include=dev && npm run build && npm run copy-resources:prod && npm run verify-build",
    "do-deploy": "npm run do-build && mkdir -p dist/.do && cp -r .do/* dist/.do/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "encrypt": "node --loader ts-node/esm scripts/encryptQuestionBanks.ts",
    "encrypt-premium": "tsx scripts/encrypt-premium-content.ts",
    "encrypt-basic": "tsx scripts/encrypt-basic-content.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "deploy:functions": "bash scripts/deploy-edge-functions.sh",
    "start": "node server.js",
    "verify-build": "node verify-build.js",
    "copy-resources": "node scripts/copy-resources.js",
    "copy-resources:prod": "node scripts/copy-resources-prod.js",
    "check-paths": "find dist/app -name index.html -exec grep -l '/src/main.tsx' {} \\; | xargs sed -i '' 's|/src/main.tsx|/app/assets/main|g'",
    "fix-app-html": "node scripts/fix-app-html.js",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build && npm run copy-resources && npm run fix-app-html && npm run verify-build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@stripe/stripe-js": "^5.6.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tailwindcss/forms": "^0.5.10",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "express": "^4.18.3",
    "fs": "^0.0.1-security",
    "helmet": "^7.1.0",
    "lucide-react": "^0.358.0",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^6.22.2",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.14",
    "vm-browserify": "^1.1.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.0.6",
    "autoprefixer": "^10.4.18",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.35",
    "supabase": "^2.12.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.3.0",
    "vitest": "^3.0.6"
  }
}
