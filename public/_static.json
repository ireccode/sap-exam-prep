{
  "root": "./",
  "routes": {
    "/app/**": {
      "type": "rewrite",
      "dest": "/app/index.html"
    },
    "/login": {
      "type": "redirect",
      "dest": "/app/login"
    },
    "/**": {
      "type": "rewrite",
      "dest": "/index.html"
    }
  },
  "headers": {
    "/**": {
      "Content-Type": "text/html; charset=utf-8"
    },
    "/app/assets/*.css": {
      "Content-Type": "text/css; charset=utf-8",
      "Cache-Control": "public, max-age=604800"
    },
    "/app/assets/*.js": {
      "Content-Type": "text/javascript; charset=utf-8",
      "Cache-Control": "public, max-age=604800"
    },
    "/images/*": {
      "Cache-Control": "public, max-age=604800"
    },
    "/*.encrypted": {
      "Content-Type": "application/octet-stream",
      "Cache-Control": "public, max-age=604800"
    }
  }
} 